    "mariadb": "^3.2.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "oracledb": "^6.2.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6",
    "tedious": "^16.6.1",
